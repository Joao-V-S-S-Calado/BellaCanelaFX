<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="584.0" prefWidth="1170.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bellacanelafx.SelecionarMesaController">
   <content>
      <TilePane fx:id="tpPane" alignment="TOP_CENTER" hgap="10.0" prefHeight="200.0" prefWidth="200.0" vgap="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <AnchorPane id="AnchorPane" fx:id="apPane" prefHeight="200.0" prefWidth="200.0" styleClass="mesa-liberada" stylesheets="@css/Mesa.css">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="10.0">
                     <children>
                        <Label style="-fx-font-size: 18;" styleClass="labelh2" text="Adicionar Mesa" />
                        <JFXButton fx:id="btAddMesa" onAction="#evtAddMesa" text="+">
                           <styleClass>
                              <String fx:value="button" />
                              <String fx:value="buttonMesaLiberadada" />
                           </styleClass>
                        </JFXButton>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
         </children>
      </TilePane>
   </content>
</ScrollPane>
